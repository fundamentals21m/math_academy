# Track all PNG screenshots with Git LFS
*.png filter=lfs diff=lfs merge=lfs -text

# Track test result artifacts
baselines/**/*.png filter=lfs diff=lfs merge=lfs -text
test-results/**/*.png filter=lfs diff=lfs merge=lfs -text
test-results/**/*.webm filter=lfs diff=lfs merge=lfs -text
test-results/**/*.zip filter=lfs diff=lfs merge=lfs -text
