        {
          "id": "10-63-2",
          "type": "exercise",
          "number": 2,
          "statement": "Compute $\\int e^{it}\\,dt$.",
          "solution": {
            "steps": [
              {
                "title": "Apply Exponential Rule",
                "content": "$\\int e^{it}\\,dt = \\frac{1}{i}e^{it} + C$"
              },
              {
                "title": "Simplify",
                "content": "$\\frac{1}{i} = \\frac{-i}{-i^2} = \\frac{-i}{1} = -i$\n\nSo $\\int e^{it}\\,dt = -ie^{it} + C$"
              }
            ],
            "conclusion": "$\\int e^{it}\\,dt = -ie^{it} + C$"
          }
